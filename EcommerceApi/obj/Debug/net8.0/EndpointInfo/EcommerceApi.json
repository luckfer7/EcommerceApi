{
  "openapi": "3.0.1",
  "info": {
    "title": "Ecommerce API",
    "version": "v1"
  },
  "paths": {
    "/register": {
      "post": {
        "tags": [
          "Auth"
        ],
        "parameters": [
          {
            "name": "Nome",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "Email",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "Senha",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
